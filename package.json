{
  "name": "nr1",
  "displayName": "nr1",
  "description": "A test",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:nr1.catalogInfo",
    "onCommand:nr1.catalogSubmit",
    "onCommand:nr1.createNerdpack",
    "onCommand:nr1.createNerdlet",
    "onCommand:nr1.createLauncher",
    "onCommand:nr1.createCatalog",
    "onCommand:nr1.runNerdpack",
    "onCommand:nr1.publishNerdpack",
    "onCommand:nr1.deployNerdpack",
    "onCommand:nr1.subscribeNerdpack",
    "onCommand:nr1.unsubscribeNerdpack",
    "onCommand:nr1.selectProfile"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets.json"
      }
    ],
    "commands": [
      {
        "command": "nr1.createNerdpack",
        "title": "Nerdpack: create a nerdpack"
      },
      {
        "command": "nr1.createNerdlet",
        "title": "Nerdpack: create a nerdlet"
      },
      {
        "command": "nr1.createLauncher",
        "title": "Nerdpack: create a launcher"
      },
      {
        "command": "nr1.subscribeNerdpack",
        "title": "Nerdpack: subscribe account to nerdpack"
      },
      {
        "command": "nr1.unsubscribeNerdpack",
        "title": "Nerdpack: unsubscribe account from nerdpack"
      },
      {
        "command": "nr1.createCatalog",
        "title": "Nerdpack: create catalog listing"
      },
      {
        "command": "nr1.catalogSubmit",
        "title": "Nerdpack: submit catalog listing"
      },
      {
        "command": "nr1.catalogInfo",
        "title": "Nerdpack: show published catalog listing info"
      },
      {
        "command": "nr1.runNerdpack",
        "title": "Nerdpack: run local development"
      },
      {
        "command": "nr1.publishNerdpack",
        "title": "Nerdpack: publish"
      },
      {
        "command": "nr1.deployNerdpack",
        "title": "Nerdpack: deploy"
      },
      {
        "command": "nr1.selectProfile",
        "title": "New Relic Profiles: select default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.44.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}
